{
    "name": "sixincode/hive-translation",
    "description": "hive-translation package",
    "keywords": [
        "sixincode",
        "laravel",
        "hive-translation"
    ],
    "homepage": "https://github.com/sixincode/hive-translation",
    "license": "MIT",
    "authors": [
        {
            "name": "sixincode",
            "email": "sixincode@6ixin.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0",
        "sixincode/modules-init": "dev-main"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.7|^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Sixincode\\HiveTranslation\\": "src",
            "Sixincode\\HiveTranslation\\Database\\Factories\\": "database/factories"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Sixincode\\HiveTranslation\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sixincode\\HiveTranslation\\HiveTranslationServiceProvider"
            ],
            "aliases": {
                "HiveTranslation": "Sixincode\\HiveTranslation\\Facades\\HiveTranslation"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
